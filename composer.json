{
  "name": "levii22/university-system",
  "version": "0.2.1",
  "date": "2023-05-16",
  "description": "UniTask is a web application for managing tasks and projects.",
  "homepage": "https://www.yiiframework.com/",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "~2.0.2",
    "yiisoft/yii2-symfonymailer": "~2.0.3",
    "hail812/yii2-adminlte3": "~1.1",
    "kartik-v/yii2-widget-select2": "^2.2",
    "firebase/php-jwt": "^6.4",
    "cranky4/change-log-behavior": "*",
    "yiisoft/yii2-twig": "^2.4",
    "notamedia/yii2-sentry": "^1.7",
    "yiisoft/yii2-queue": "^2.3",
    "kartik-v/yii2-password": "^1.5",
    "kartik-v/yii2-grid": "^3.5",
    "kartik-v/yii2-bootstrap5-dropdown": "^1.0",
    "kartik-v/yii2-mpdf": "dev-master",
    "kartik-v/yii2-widget-datetimepicker": "dev-master",
    "kartik-v/yii2-widget-fileinput": "dev-master",
    "kartik-v/yii2-editable": "dev-master",
    "ext-zip": "*",
    "php-curl-class/php-curl-class": "^9.14",
    "kartik-v/yii2-widget-growl": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "~2.0.0",
    "phpunit/phpunit": "~9.5.0",
    "codeception/codeception": "^5.0.0 || ^4.0",
    "codeception/lib-innerbrowser": "^3.0 || ^1.1",
    "codeception/module-asserts": "^3.0 || ^1.1",
    "codeception/module-yii2": "^1.1",
    "codeception/module-filesystem": "^2.0 || ^1.1",
    "codeception/verify": "^2.2",
    "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
